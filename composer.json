{
  "name": "jivoo/jivoo",
  "description": "Jivoo web application framework.",
  "license": "MIT",
  "type": "library",
  "homepage": "http://nielssp.dk",
  "keywords": ["framework"],
  "authors": [
    {
      "name": "Niels Sonnich Poulsen",
      "email": "niels@nielssp.dk",
      "homepage": "http://nielssp.dk",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "jivoo@nielssp.dk"
  },
  "autoload": {
    "psr-4": {"Jivoo\\": "src/"},
    "files": ["src/bootstrap.php"]
  },
  "autoload-dev": {
    "psr-4": {"Jivoo\\": "tests/"}
  },
  "require": {
    "php": ">=5.3.0",
    "ext-json": "*",
    "psr/log": "1.0",
    "composer/semver": "^1.2"
  },
  "suggest": {
    "ezyang/htmlpurifier": "Required by the Content module.",
    "jivoo/simplehtmldom": "Allows the use of compiled HTML templates."
  },
  "require-dev": {
    "phpunit/phpunit": "5.0.*",
    "squizlabs/php_codesniffer": "2.*",
    "apigen/apigen": "dev-master"
  }
}
